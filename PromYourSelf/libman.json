{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "select2@4.0.13",
      "destination": "wwwroot/vendor/select2/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@3.1.6",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.js"
      ]
    },
    {
      "library": "limonte-sweetalert2@7.33.1",
      "destination": "wwwroot/js/sweetAlert/"
    },
    {
      "library": "select2-bootstrap-theme@0.1.0-beta.10",
      "destination": "wwwroot/vendor/select2-bootstrap/select2-bootstrap-theme/"
    }
  ]
}